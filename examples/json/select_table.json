/// like condition
{
"operation": "select",
  "table": "users",
  "use_transaction": true,
  "where":{
      "username": {
          "operator": "like",
          "value": "%7"
      }
  },
  "pagination": {
    "page": 1,
    "pageSize": 10
  }

}

/// eq condition
{
"operation": "select",
  "table": "users",
  "where":{
      "username": {
          "operator": "eq",
          "value": "john_deo7"
      }
  },
  "pagination": {
    "page": 1,
    "pageSize": 10
  }

}

/// gt condition
{
"operation": "select",
  "table": "users",
  "where":{
      "id": {
          "operator": "gt",
          "value": "7"
      }
  },
  "pagination": {
    "page": 1,
    "pageSize": 10
  }

}